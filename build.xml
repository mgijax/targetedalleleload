<?xml version="1.0"?>
<!-- This XML script is used to build the targetedallele product.
-->
<project name="targetedalleleload" default="compile" basedir=".">

   <!-- Get the system environment variables and define any other system
        properties needed by the build.
   -->
   <property environment="env"/>
   <property name="jar.name" value="targetedalleleload"/>
   <property name="classpath" value="${env.MGI_JAVALIB}/core.jar:${env.MGI_JAVALIB}/dbsmgd.jar:${env.MGI_JAVALIB}/dbsrdr.jar:${env.MGI_JAVALIB}/dla.jar:${env.JAVALIB}/junit.jar"/>

   <!-- Import the universal targets.
   -->
   <import file="${env.MGICONFIG}/etc/build.lib.targets.xml"/>

   <!-- Define product specific targets.
   -->
   <target name="buildall" description="Executes task 'all'"
           depends="all, link">
   </target>
</project>
